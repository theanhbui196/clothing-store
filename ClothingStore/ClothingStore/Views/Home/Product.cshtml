@model IEnumerable<ClothingStore.Data.Product>
@{
    ViewBag.Title = "Product";
}

<table>
    <thead>
        <tr>
            <th>ProductName</th>
            <th>Selling Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.ProductName</td>
                <td>@product.SellingPrice</td>
            </tr>
        }
    </tbody>
</table>

